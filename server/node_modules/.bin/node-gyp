#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/OneDrive/Documents/repos/foodswipe-starters/server/node_modules/node-gyp/bin/node_modules:/mnt/d/OneDrive/Documents/repos/foodswipe-starters/server/node_modules/node-gyp/node_modules:/mnt/d/OneDrive/Documents/repos/foodswipe-starters/server/node_modules:/mnt/d/OneDrive/Documents/repos/foodswipe-starters/node_modules:/mnt/d/OneDrive/Documents/repos/node_modules:/mnt/d/OneDrive/Documents/node_modules:/mnt/d/OneDrive/node_modules:/mnt/d/node_modules:/mnt/d/OneDrive/Documents/repos/foodswipe-starters/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/OneDrive/Documents/repos/foodswipe-starters/server/node_modules/node-gyp/bin/node_modules:/mnt/d/OneDrive/Documents/repos/foodswipe-starters/server/node_modules/node-gyp/node_modules:/mnt/d/OneDrive/Documents/repos/foodswipe-starters/server/node_modules:/mnt/d/OneDrive/Documents/repos/foodswipe-starters/node_modules:/mnt/d/OneDrive/Documents/repos/node_modules:/mnt/d/OneDrive/Documents/node_modules:/mnt/d/OneDrive/node_modules:/mnt/d/node_modules:/mnt/d/OneDrive/Documents/repos/foodswipe-starters/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-gyp/bin/node-gyp.js" "$@"
else
  exec node  "$basedir/../node-gyp/bin/node-gyp.js" "$@"
fi
